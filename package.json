    "express-session": "^1.16.2",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "pg-native": "^3.0.0",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
